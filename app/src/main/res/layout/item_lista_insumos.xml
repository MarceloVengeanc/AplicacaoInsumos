<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txtAplic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%1$d"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/txtDp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%1$d"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <TextView
        android:id="@+id/txtProg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%1$d"
        android:textSize="18sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Prog: "
        android:textSize="18sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtprocedencia" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="Aplic:"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/txtProg"
        app:layout_constraintTop_toBottomOf="@+id/txtNomeInsumo" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="Dep:"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/txtAplic"
        app:layout_constraintTop_toBottomOf="@+id/txtNomeInsumo" />

    <TextView
        android:id="@+id/txtprocedencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Insumo: "
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtprocedencia2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:text="-"
        android:textStyle="bold"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/txtCodInsumo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtCodInsumo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%1$d"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtprocedencia"
        app:layout_constraintStart_toEndOf="@+id/txtprocedencia"
        app:layout_constraintTop_toTopOf="@+id/txtprocedencia"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/txtNomeInsumo"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="%1$d"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtprocedencia2"
        app:layout_constraintStart_toEndOf="@+id/txtprocedencia2"
        app:layout_constraintTop_toTopOf="@+id/txtprocedencia2" />

</androidx.constraintlayout.widget.ConstraintLayout>